#include <iostream>
using namespace std;

// Функция сортировки вставками
void insertionSort(int arr[], int n) {
    for (int i = 1; i < n; ++i) {           // начинаем со второго элемента
        int key = arr[i];                   // запоминаем текущий элемент
        int j = i - 1;                      // индекс предыдущего элемента

        // Двигаемся назад, сдвигая большие элементы вправо,
        // пока не найдём подходящее место для 'key'
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            --j;
        }
        arr[j + 1] = key;                  // размещаем ключ на нужное место
    }
}

// Вспомогательная функция для печати массива
void printArray(const int arr[], int size) {
    for (int i = 0; i < size; ++i)
        cout << arr[i] << " ";
    cout << endl;
}

int main() {
    int arr[] = {12, 11, 13, 5, 6};       // исходный массив
    int n = sizeof(arr) / sizeof(arr[0]);  // размер массива

    cout << "Исходный массив:\n";
    printArray(arr, n);

    insertionSort(arr, n);                 // сортируем массив

    cout << "\nОтсортированный массив:\n";
    printArray(arr, n);

    return 0;
}
