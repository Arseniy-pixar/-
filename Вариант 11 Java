import java.util.*;

public class Main {
    public static void main(String[] args) {
        String s = "ABC";
        List<String> result = new ArrayList<>();
        generatePermutations(result, "", s);
        
        // Печать результатов
        System.out.println(result);
    }

    private static void generatePermutations(List<String> results, String prefix, String remaining) {
        int n = remaining.length();
        if (n == 0) { // Базовый случай: строка пустая
            results.add(prefix); // Добавляем найденную перестановку
        } else {
            for (int i = 0; i < n; i++) {
                char currentChar = remaining.charAt(i);
                String before = remaining.substring(0, i);
                String after = remaining.substring(i + 1, n);
                
                // Рекурсия с уменьшением оставшейся части строки
                generatePermutations(results, prefix + currentChar, before + after);
            }
        }
    }
}
