#include <iostream>
#include <string>
using namespace std;

void swap(char &a, char &b) {
    char temp = a;
    a = b;
    b = temp;
}

// Рекурсивная функция для генерации перестановок
void generatePermutations(string str, int startIndex, int endIndex) {
    if (startIndex == endIndex) {
        cout << str << endl;
    } else {
        for(int i=startIndex;i<=endIndex;i++){
            swap(str[startIndex], str[i]);
            generatePermutations(str, startIndex+1, endIndex);
            swap(str[startIndex], str[i]); // Откатываем изменение
        }
    }
}

int main() {
    string inputString = "ABC";
    int size = inputString.size();
    generatePermutations(inputString, 0, size-1);
    return 0;
}
